  <Code>Function StripTags(ByVal HTML As String) As String
    ' Removes tags from passed HTML
    Dim objRegEx As _
        System.Text.RegularExpressions.Regex
    Dim sb = new System.Text.StringBuilder(objRegEx.Replace(HTML, "&lt;[^&gt;]*&gt;", ""))
    sb.Replace("&amp;nbsp;"," ")
    sb.Replace("&amp;lt;","&lt;")
    sb.Replace("&amp;gt;","&gt;")
    sb.Replace("&amp;quot;","""")
    sb.Replace("&amp;amp;","&amp;")
    sb.Replace("&amp;#8220;","""")
    sb.Replace("&amp;#8221;","""")
    sb.Replace("&amp;#8226;", "-")
    sb.Replace("&amp;#8211;","-")
    sb.Replace("&amp;#8217;","'")
    sb.Replace("&amp;#8212;",",")
    sb.Replace("&amp;#167;","")
    sb.Replace("&amp;#x000D","")
    sb.Replace("&amp;#x000A","")
    Return sb.ToString()
End Function</Code>
