      <Query>
        <DataSourceName>HealthWare_Corporation_MSCRM</DataSourceName>
        <CommandText>SELECT    CAST(Activity.ActivityID AS nvarchar(80)) AS A_ID ,CAST(LeadBase.LeadID AS nvarchar(80))  as ID,LeadBase.CompanyName AS Name, Rtrim(ObjectTypeCodes.ObjectName) as ObjectName, ObjectTypeCodes_1.ObjectName AS Activity_Type, LeadBase.ContactId, 
                      Activity.CreatedOn, Activity.Subject, Activity.CreatedByName, LeadBase.StatusCode, isnull(Activity.StatusCode,1) AS Activity_Status, 
                      SystemUserBase.LastName, Activity.Description
FROM         activity Inner JOIN
                      leadbase ON activity.objectid = leadbase.leadid INNER JOIN
                      ObjectTypeCodes ObjectTypeCodes_1 ON Activity.ActivityTypeCode = ObjectTypeCodes_1.ObjectTypeCode INNER JOIN
                      ObjectTypeCodes ON Activity.ObjectTypeCode = ObjectTypeCodes.ObjectTypeCode INNER JOIN
                      SystemUserBase ON Activity.OwnerID = SystemUserBase.SystemUserId
WHERE     activity.objecttypecode IN (4)
UNION ALL
SELECT    CAST(Activity.ActivityID AS nvarchar(80)) AS A_ID ,CAST(OpportunityBase.OpportunityId AS nvarchar(80)) AS ID,OpportunityBase.Name, Rtrim(ObjectTypeCodes.ObjectName) as ObjectName, ObjectTypeCodes_1.ObjectName AS Activity_Type, OpportunityBase.ContactId, 
                      Activity.CreatedOn, Activity.Subject, Activity.CreatedByName, OpportunityBase.StatusCode, isnull(Activity.StatusCode,1) AS Activity_Status, 
                      SystemUserBase.LastName, Activity.Description
FROM         activity Inner JOIN
                      opportunitybase ON activity.objectid = opportunitybase.opportunityid INNER JOIN
                      ObjectTypeCodes ObjectTypeCodes_1 ON Activity.ActivityTypeCode = ObjectTypeCodes_1.ObjectTypeCode INNER JOIN
                      ObjectTypeCodes ON Activity.ObjectTypeCode = ObjectTypeCodes.ObjectTypeCode INNER JOIN
                      SystemUserBase ON Activity.OwnerID = SystemUserBase.SystemUserId
WHERE     activity.objecttypecode IN (3) order by activity.createdon desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
