      <Query>
        <DataSourceName>Adventure_Works_Cycle_MSCRM</DataSourceName>
        <CommandText>
        SELECT     
			LeadId, 
			CompanyName, 
			Address1_City, 
			Address1_StateOrProvince, 
			Address1_PostalCode, 
			Address1_Telephone1, 
			ContactIdName, 
			EMailAddress1, 
            EstimatedValue, 
            LeadSourceCode, 
		CASE LeadSourceCode 
			WHEN 1 THEN 'Advertisement' 
			WHEN 2 THEN 'Employee Referral' 
			WHEN 3 THEN 'External Referral'
			WHEN 4 THEN 'Partner'
			WHEN 5 THEN 'Public Relations'
			WHEN 6 THEN 'Seminar'
			WHEN 7 THEN 'Trade Show'
			WHEN 8 THEN 'Web' 
			WHEN 9 THEN 'Word of Mouth'
			WHEN 10 THEN 'Other' 
			WHEN 11 THEN 'Other'
		END AS leadsourcecodename,
 		OwnerIdName, 
		EstimatedCloseDate, 1 AS LeadCount
FROM         Lead
ORDER BY CompanyName</CommandText>
      </Query>
    </DataSet>
  </DataSets>
  <Author>Microsoft</Author>
  <Code>Function translateLeadSourceCode(code as string)
	if code = "1" then
	return "Advertisement"
	else if code = "2" then
	return "Employee Referral"
	else if code = "3" then
	return "External Referral"
	else if code = "4" then
	return ""
	else if code = "5" then
	return ""
	else if code = "6" then
	return ""
	else if code = "7" then
