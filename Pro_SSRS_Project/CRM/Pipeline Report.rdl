      <Query>
        <DataSourceName>Adventure_Works_Cycle_MSCRM</DataSourceName>
        <CommandText>declare @SQLString varchar(8000)
set @SQLString = 'SELECT     
	customeridname, 
	name AS Opportunityname, 
	estimatedvalue, 
	estimatedclosedate, 
	closeprobability, 
	originatingleadidname, 
	owneridname,
	statecode,
CASE  statecode 
      WHEN 0 THEN' + '''Open''' + '
      WHEN 1 THEN' + '''Won''' + '
      WHEN 2 THEN' + '''Lost''' + '
END as statecodename, 
      statuscode,
CASE
      WHEN statuscode = 1 AND statecode = 0  THEN' + '''In Progress''' + '
      WHEN statuscode = 2 AND statecode = 0  THEN' + '''On Hold''' + '
      WHEN statuscode = 3 AND statecode = 1  THEN' + '''Won''' + '
      WHEN statuscode = 4 AND statecode = 2  THEN' + '''Cancelled''' + '
      WHEN statuscode = 5 AND statecode = 2  THEN' + '''Out Sold''' + '
	ELSE CONVERT(varchar(10), statuscode)
END as statuscodename,  
CASE opportunityratingcode 
      WHEN 1 THEN' + '''Hot''' + '
      WHEN 2 THEN' + '''Warm''' + '
      WHEN 3 THEN' + '''Cold''' + '
END as opportunityratingcodename,
	actualvalue 
FROM opportunity Opportunity 
ORDER BY ' + @Sort + ' ' + @Direction
exec (@SQLString)</CommandText>
